<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Add Materi</title>
  <%- include('./partials/_bootstrap') %>
</head>
<%- include('./partials/_navbar') %>
<body>
    <div class="container mt-5">
        <h2>User List</h2>
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Username</th>
                    <th>Email</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                <% data.forEach(el => { %>
                    <tr>
                        <td>el.username</td>
                        <td>el.email</td>
                        <td>
                            <a href="/userLists/delete/<%= el.id %>" type="button" class="btn btn-sm btn-outline-danger">Delete</a>
                        </td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </div>
</body>
</html>